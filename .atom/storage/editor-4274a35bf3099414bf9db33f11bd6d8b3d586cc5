 tstop. */\n  //###########################################################################\n  {\n    double tstart; // current time\n    double tstop;  // evolve u to this time\n\n    // Grab status of current time step\n    braid_PhiStatusGetTstartTstop(status, &tstart, &tstop);\n\n    return 0;\n  }\n\n\n  //###########################################################################\n  int my_Init( braid_App app, double t, braid_Vector *u_ptr )\n  // Tells XBraid, how to initialize a vector at time t\n  //###########################################################################\n  {\n    // Allocate memory on the heap\n    my_Vector *u;       // declare a pointer to a my_Vector variable\n    u = new my_Vector;  // allocate memory on the heap and set pointer to it\n\n    //Set initial value as first guess\n    initial_data(u->sol);\n\n    // set pointer for output\n    *u_ptr = u;\n\n    return 0;\n  }\n\n\n  //###########################################################################\n  int my_Clone( braid_App app, braid_Vector u, braid_Vector *v_ptr )\n  // Tells XBraid, how to clone a vector u into a new vector, *v_ptr\n  //###########################################################################\n  {\n    // Allocate memory\n    my_Vector *v;      // declare a pointer to a my_vector variable\n    v = new my_Vector;  // allocate memory on the heap and set pointer to it\n\n    // Copy values from u to v\n    v->sol.y = u->sol.y;\n    v->sol.y_p = u->sol.y_p;\n    v->sol.z = u->sol.z;\n    v->sol.z_p= u->sol.z_p;\n    for (int m=0; m<M; m++)\n    {\n      v->sol.u[m] = u->sol.u[m];\n      v->sol.u_p[m] = u->sol.u_p[m];\n    }\n    v->sol.dt = u->sol.dt;\n\n    // set pointer for output\n    *v_ptr = v;\n\n    return 0;\n  }\n\n\n  //###########################################################################\n  int my_Free( braid_App app, braid_Vector u )\n  // Tells XBraid, how to free a vector\n  //###########################################################