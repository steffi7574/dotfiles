{"version":1,"windowDimensions":{"x":4,"y":22,"width":952,"height":1028,"maximized":false},"grammars":{"grammarOverridesByPath":{}},"project":{"deserializer":"Project","paths":["/home/sguenther/Numerics/PinT","/home/sguenther/Software/braid"],"buffers":[{"text":"#include <structures.hpp>\n#include <math.h>\n#include <iostream>\n#include <functions.hpp>\n#include <assert.h>\n#include <lsstypes.hpp>\n#include <globalconst.hpp>\n\n\n//######################################################################################\nvoid time_stepping(SolutionVars &sol, DiscretizationVars &disc, ConvergenceVars &conv)\n// Applies a time stepping scheme to solve the advection-diffusion equation with\n// lorenz boundary.\n// Trapezoidal rule for time discretization, LUD and central diff. for first and second\n// derivative\n// Relaxed funtional iterations for implicit equations.\n//######################################################################################\n{\n\n  SolutionVars *sol_p;\n  sol_p = new SolutionVars;\n\n  //shift solution in time\n  sol_p->y = sol.y;\n  sol_p->z = sol.z;\n  for (int k = 0; k < M; k++)\n  {\n    sol_p->u[k] = sol.u[k];\n  }\n\n  // initialize the iteration at current time\n  initial_data(sol);\n\n  //functional iteration for implicit equation\n  for(int k = 0; k < maxiter_functional; k++)\n  {\n     // std::cout<< ts <<\" \"<< k <<\" \"<< sol.z <<\"\\n\";\n     //update solution and residuum at time step ts\n     FixedPointSolverG(sol, *sol_p, disc, conv);\n\n    //convergence control\n    conv.normres = sqrt(conv.res[0] + conv.res[1]);\n    if (conv.normres < abstol_functional)\n    {\n      break;\n    }\n  }\n\n  delete sol_p;\n\n}\n\n\n//######################################################################################\nvoid FixedPointSolverG(SolutionVars &sol, SolutionVars &sol_p,\n                      DiscretizationVars &disc, ConvergenceVars &conv)\n// performes one step of the contractive fixed point solver at current time step\n//######################################################################################\n{\n\n  RealType res_vdp[dimode];\n  RealType res_advdiff[M];\n\n  // right hand side for ODE and Advection-Diffusion\n  residuum_vdp(sol, sol_p, disc, res_vdp);\n  residuum_ad(sol, sol_p, disc, res_advdiff);\n\n  // convergence monitoring\n  conv.res[0] = linalg_norm_square(res_vdp,dimode);\n  conv.res[1] = linalg_norm_square(res_advdiff,M) / M;\n\n  // update with damped functional iteration\n  sol.y = sol.y - urfy * res_vdp[0];\n  sol.z = sol.z - urfz * res_vdp[1];\n  for (int k = 0; k<M; k++)\n  {\n    sol.u[k] = sol.u[k] - urfu * res_advdiff[k];\n  }\n\n}\n\n\n\n//######################################################################################\nvoid vdp_eval(RealType y, RealType z, RealType* rhs)\n// return right hand side of Van-der-Pol ODE.\n// IN: x,y,z   OUT: rhs[2]\n//######################################################################################\n{\n\n  rhs[0] = z;\n  rhs[1] = - y + beta_vdp * (1.0 - pow(y,2)) * z ;\n\n}\n\n\n//######################################################################################\nvoid residuum_vdp(SolutionVars& sol, SolutionVars& sol_p,\n                  DiscretizationVars &disc, RealType* res)\n// computes residuum of the discretized Van-der-Pol ODE (trapezoidal rule)\n// IN: struct solution,  OUT: res[2]\n//######################################################################################\n{\n  // right hand side of vdp at actual and previous time step\n  RealType rhs[dimode], rhs_p[dimode];\n  vdp_eval(sol.y, sol.z, rhs);\n  vdp_eval(sol_p.y, sol_p.z, rhs_p);\n\n  // compute residuum\n  res[0] = sol.y - sol_p.y - 0.5 * disc.dt * (rhs[0] + rhs_p[0]);\n  res[1] = sol.z - sol_p.z - 0.5 * disc.dt * (rhs[1] + rhs_p[1]);\n}\n\n\n//######################################################################################\nRealType ad_eval(RealType uww, RealType uw, RealType ui, RealType ue)\n// returns right hand side of discrete adv.-diff. equation\n// IN: uww = u_{i-2}, uw = u_{i-1}, ui = u_i, ue = u_{i+1}\n//######################################################################################\n{\n  RealType rhs;\n\n  rhs = -a/2 * (3.0 * ui - 4*uw + uww) + mu / dx * (ue - 2* ui + uw);\n\n  return rhs;\n}\n\n\n//######################################################################################\nvoid residuum_ad(SolutionVars& sol, SolutionVars& sol_p,\n                DiscretizationVars& disc, RealType* res)\n//  returns residuum of the discretized Advection-Diffusion equation (trapezoidal rule,\n//  second order upwind (LUD) and central diff. for first and second spatial derivative\n//######################################################################################\n{\n\n  RealType rhs;\n  RealType rhs_p;\n\n  // first element next to left boundary\n  rhs = ad_eval(2.0 * amp * sol.z - sol.u[0], amp * (sol.z), sol.u[0], sol.u[1]);\n  rhs_p = ad_eval(2.0 * amp * sol_p.z - sol_p.u[0], amp * (sol_p.z), sol_p.u[0], sol_p.u[1]);\n  res[0] = sol.u[0]  - sol_p.u[0] - 0.5 * disc.dt / dx * ( rhs + rhs_p );\n\n  // second element\n  rhs = ad_eval(amp * (sol.z), sol.u[0], sol.u[1],  sol.u[2]);\n  rhs_p = ad_eval(amp * (sol_p.z), sol_p.u[0], sol_p.u[1],  sol_p.u[2]);\n  res[1] = sol.u[1]  - sol_p.u[1] - 0.5 * disc.dt / dx * ( rhs + rhs_p );\n\n  // inside the domain\n  for (int k = 2; k < M-1; k++)\n  {\n    rhs = ad_eval(sol.u[k-2], sol.u[k-1], sol.u[k], sol.u[k+1]);\n    rhs_p = ad_eval(sol_p.u[k-2], sol_p.u[k-1], sol_p.u[k], sol_p.u[k+1]);\n    res[k] = sol.u[k]  - sol_p.u[k] - 0.5 * disc.dt / dx * ( rhs + rhs_p );\n  }\n\n  // right boundary (linear extrapolation to ghost layer cell)\n  rhs = ad_eval(sol.u[M-3], sol.u[M-2], sol.u[M-1], 2*sol.u[M-1] - sol.u[M-2]);\n  rhs_p = ad_eval(sol_p.u[M-3], sol_p.u[M-2], sol_p.u[M-1], 2*sol_p.u[M-1] - sol_p.u[M-2]);\n  res[M-1] = sol.u[M-1]  - sol_p.u[M-1] - 0.5 * disc.dt / dx * ( rhs + rhs_p );\n\n}\n","defaultMarkerLayerId":"0","markerLayers":